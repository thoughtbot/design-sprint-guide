---
eleventyExcludeFromCollections: true
---

<h1>Phases</h1>

<ul class="phase-rows" id="phases">
  {% for phase in collections.phase %}
  <li>
    <a href="{{ phase.data.page.url }}">
      <img src="#" alt="">
      <span class="eyebrow">Phase {{ phase.data.number }}</span>
      <h3>{{ phase.data.title }}</h3>
    </a>
  </li>
  {% endfor %}
</ul>
